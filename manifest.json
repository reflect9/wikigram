{

  "name" : "WikiGram",
  "version" : "1.0",
  "description" : "wikigram extractor",
  "background" : {
    "scripts": ["js/lib/jquery.min.js",
                "js/lib/underscore-min.js",
                "js/lib/purl.js",
                "background.js"]
  },
  "browser_action" :
  {
    "default_icon" : "sandwich-19.png",
    "default_title" : "open wikigram!"
  },
  "content_scripts" : [
    {
      "matches" : ["<all_urls>"],
      "js" : [	"js/lib/jquery.min.js",
      			"js/lib/jquery.mousewheel.min.js",
      			"js/lib/underscore-min.js",
      			"js/lib/utils.js",
            "js/lib/bootstrap-tooltip.js",
            "js/lib/bootstrap-popover.js",
            "js/lib/purl.js",  
            "js/lib/jquery-ui-interactions-widgets.js",                       
            "js/wg.js",
      			"js/column.js",
            "js/cell.js",
      			"js/generator.js",
      			"js/inspector.js",
      			"js/operation.js",
            "js/operationPopup.js",
            "js/procedure.js",
            "js/loader.js",
      			"js/program.js",
      			"js/candidates.js",
      			"js/selectionBox.js",
      			"js/sheet.js",
      			"js/widget.js",
            "contentscript.js"
      		],
      "css": [  "css/ui-lightness/jquery-ui-1.10.2.custom.min.css",
                "css/wikiGram.css"
              ],
      "run_at" : "document_idle",
      "all_frames" : false
    }
  ],
  "icons" : {
    "48" : "sandwich-48.png",
    "128" : "sandwich-128.png"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "contextMenus",
    "notifications",
    "storage",
    "http://*/*", "https://*/*",
    "http://*/", "https://*/"
  ],
  "web_accessible_resources": [
    "css/image/glyphicons-halflings.png"
  ],
  "manifest_version": 2
}